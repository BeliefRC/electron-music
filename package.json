{
  "name": "electron-quick-start",
  "version": "1.0.0",
  "description": "A minimal Electron music application",
  "main": "main.js",
  "scripts": {
    "start": "nodemon --exec \"electron .\"",
    "dist": "electron-builder --win --x64"
  },
  "build": {
    "appId": "com.xxx.app",
    "productName": "rc-music-player",
    "mac": {
      "target": ["dmg","zip"]
    },
    "win": {
      "target": ["nsis","zip"]
    }
  },
  "repository": "https://github.com/BeliefRC/electron-music.git",
  "keywords": [
    "Electron"
  ],
  "author": "BeliefRc",
  "devDependencies": {
    "electron": "^11.5.0",
    "electron-builder": "^21.2.0",
    "nodemon": "^1.19.2"
  },
  "dependencies": {
    "bootstrap": "^4.3.1",
    "electron-store": "^5.0.0",
    "uuid": "^3.3.3"
  }
}
